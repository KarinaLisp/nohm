{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "arrowFunctions": true,
      "generators": true,
      "classes": true
    },
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off"
  }
}
