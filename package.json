    "@types/uuid": "^3.4.3",
    "async": "^1.2.1",
    "concurrently": "^3.5.0",
    "nodemon": "^1.12.1",
    "nodeunit": "^0.9.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
